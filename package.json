{
  "name": "viki-scraper",
  "version": "1.0.0",
  "description": "",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "./lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "dev": "esbuild --watch src/index.ts --format=esm --outdir=lib --sourcemap",
    "build": "esbuild src/index.ts --format=esm --outdir=lib --minify && npm run types",
    "types": "tsc --declaration --emitDeclarationOnly",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src --ext .ts --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eliutdev/viki-scraper-js.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eliutdev/viki-scraper-js/issues"
  },
  "homepage": "https://github.com/eliutdev/viki-scraper-js#readme",
  "dependencies": {
    "linkedom": "^0.14.7"
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "esbuild": "0.14.38",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "jest": "27.5.1",
    "prettier": "2.6.2",
    "ts-jest": "27.1.4",
    "typescript": "4.6.3"
  }
}
